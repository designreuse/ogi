<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.0">
	<xsd:element name="TRANSFERT">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Cette balise décrit l agence qui transfère les biens.  -->
				<xsd:element name="AGENCE" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Numéro FNAIM de l'agence. Un numéro d'adhérent FNAIM est compris entre 1 et 599 999  -->
							<xsd:element name="ADH_NUM">
								<xsd:simpleType>
									<xsd:restriction base="xsd:int">
										<xsd:maxInclusive value="600000" fixed="true"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- v2.3 -->
							<!--
							Le numéro SIREN dans le cas d’un agence principale / Le numéro SIRET dans le cas d’une succursale. 
							Cet identifiant nous permet de faire le rapprochement entre les informations des agences reçues et nos adhérents. 
							Tout mandat appartenant à une agence non identifiable par le numéro SIRET ou SIREN transmis ou non sera rejeté. -->
							<xsd:element name="ID_ENTREPRISE" type="xsd:string" minOccurs="0"/>
							<!-- Cette balise décrit la liste des négociateurs de l'agence qui transfère les biens. Cette balise est répétée autant de fois qu'il existe de négociateurs. -->
							<xsd:element name="NEGOCIATEUR" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:all>
                                        <!-- Identifiant unique du négociateur. Taille maximum : 50 caractères. -->
										<xsd:element name="NUM">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
                                        <!-- Civilité du négociateur. (voir dictionnaire des références) -->
										<xsd:element name="CIVILITE" type="xsd:int"/>
                                        <!-- Nom du négociateur. Taille maximum : 40 caractères. -->
										<xsd:element name="NOM">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
                                        <!-- Prénom du négociateur. Taille maximum : 40 caractères. -->
										<xsd:element name="PRENOM">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
                                        <!-- Téléphone du négociateur. Taille maximum : 15 caractères. -->
										<xsd:element name="TELEPHONE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Portable du négociateur. Taille maximum : 15 caractères. -->
										<xsd:element name="PORTABLE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Email du négociateur. Taille maximum : 50 caractères -->
										<xsd:element name="EMAIL" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>		
				<!-- Cette balise décrit un bien à transférer vers les serveurs FNAIM. Cette balise est le point d'entrée de la description d'un bien. Sa définition conditionnera les balises filles conformément à l'action de création, modification ou suppression du bien. Cette balise est répétée autant de fois qu'il existe de biens à transférer. -->
				<xsd:element name="BIEN" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:all>
							<!-- Cette balise contient les informations du ou des propriétaires de l’affaire. Cette balise est répétée autant de fois qu'il existe de propriétaires. Elle doit être contenue dans une balise BIEN  -->
							<xsd:element name="PROPRIETAIRE" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- Numéro de la fiche du propriétaire. Taille maximum : 50 caractères.  -->
										<xsd:element name="PRO_NUM">
										<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>	
                                        <!-- Civilité du négociateur. (voir dictionnaire des références) -->
										<xsd:element name="CIVILITE" type="xsd:int"/>
                                        <!-- Nom du négociateur. Taille maximum : 40 caractères. -->
										<xsd:element name="NOM">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
                                        <!-- Prénom du négociateur. Taille maximum : 40 caractères. -->
										<xsd:element name="PRENOM">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Ligne 1 de l'adresse. Taille maximum : 40 caractères.  -->
										<xsd:element name="ADRESSE1">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Ligne 2 de l'adresse. Taille maximum : 40 caractères.  -->
										<xsd:element name="ADRESSE2" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Code postal de la commune. Taille maximum : 5 chiffres -->
										<xsd:element name="CP">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{5})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Code INSEE de la commune. Ce champ n'est pas obligatoire aujourd'hui mais le sera plus tard. Taille maximum : 5 chiffres. -->
										<xsd:element name="CODE_INSEE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{5})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Ville du propriétaire. Taille maximum : 50 caractères. -->
										<xsd:element name="VILLE">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Code du pays -->
										<xsd:element name="PAYS" type="xsd:int"/>
										<!-- Telephone Taille maximum : 15 caractères. -->
										<xsd:element name="TELEPHONE_DOMICILE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Telephone Taille maximum : 15 caractères. -->
										<xsd:element name="TELEPHONE_BUREAU" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Telephone Taille maximum : 15 caractères. -->
										<xsd:element name="TELEPHONE_PORTABLE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Email du négociateur. Taille maximum : 50 caractères -->
										<xsd:element name="EMAIL" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- commentaires Taille maximum : 255 caractères -->
										<xsd:element name="COMMENTAIRES" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="255"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="INFO_GENERALES">
								<xsd:complexType>
									<xsd:all>
                                        <!-- Numéro FNAIM de l'agence. Un numéro d'adhérent FNAIM est compris entre 1 et 599 999  -->
										<xsd:element name="ADH_NUM">
											<xsd:simpleType>
												<xsd:restriction base="xsd:int">
													<xsd:maxInclusive value="600000" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- v2.3 -->
										<!--
										  Le numéro SIREN dans le cas d’un agence principale / Le numéro SIRET dans le cas d’une succursale. 
										  Cet identifiant nous permet de faire le rapprochement entre les informations des agences reçues et nos adhérents. 
										  Tout mandat appartenant à une agence non identifiable par le numéro SIRET ou SIREN transmis ou non sera rejeté. -->
										<xsd:element name="ID_ENTREPRISE" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AFF_NUM">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>			              
										<!-- Code du négociateur. Taille maximum : 50 caractères.  -->                                                                  
										<xsd:element name="CODE_NEGOCIATEUR" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Date de création de l’affaire.  -->
										<xsd:element name="DATE_CREATION">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Cette balise n’est présente que lors de la réception. A l’envoi vers le serveur la date de modification correspond au jour  
                                        de la transaction.  -->
										<xsd:element name="DATE_MODIFICATION">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="DATE_ECHEANCE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Gestion de l’affichage sur Internet. Si false’ l’affaire ne sera pas visible sur le site Internet  -->
										<xsd:element name="TRANSFERT_PUBLIC" type="BoolType"/>
										<!--  -->
										<xsd:element name="VISIBLE_TOUTES_AGENCES" type="BoolType"/>
                                        <!-- Affaire réservée aux collectivités.   -->
										<xsd:element name="AFFAIRE_RESERVEE" type="BoolType" minOccurs="0"/>
                                        <!-- Consommation d’énergie en kWhEP/m²/an.   -->
										<xsd:element name="DPE" type="xsd:decimal" minOccurs="0"/>
                 						<!-- Classification de la consommation d’énergie.   -->		
										<xsd:element name="ETIQUETTE_DPE" type="xsd:string" minOccurs="0"/>
										<!-- Estimation des émissions : kg éqCO2/m²/an.   -->
										<xsd:element name="GES" type="xsd:decimal" minOccurs="0"/>
										<!-- Classification des émissions GES.   -->
										<xsd:element name="ETIQUETTE_GES" type="xsd:string" minOccurs="0"/>
										<!-- La valeur DPE n’est pas obligatoire.   -->
										<xsd:element name="DPE_NON_OBLIGATOIRE" type="BoolType" minOccurs="0"/>
										<!-- Contact en charge de l’annonce.   -->
										<xsd:element name="CONTACT_INTERNET" type="xsd:string" minOccurs="0"/>
										<!-- Telephone Taille maximum : 15 caractères. -->
										<xsd:element name="TELEPHONE_INTERNET" type="xsd:string" minOccurs="0"/>
										<!-- Le téléphone à afficher sur l’annonce.   -->
										<xsd:element name="EMAIL_INTERNET" type="xsd:string" minOccurs="0"/>
										<!-- v2.3 -->
										<!--  L’activité est exercée sous le statut d’agent commercial (0 : non, 1 : oui)   -->
										<xsd:element name="AGENT_STATUT_COMMERCIAL" type="xsd:int" minOccurs="0"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations concernant la location du bien. Elle peut être couplée avec la balise <VENTE> dans le cas d’une vente location. Elle doit être contenue dans une balise BIEN -->
							<xsd:element name="LOCATION" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- Balise mère incluant le type de loyer ainsi que sa valeur (il est à noter que au moins un des champs enfants est 
                                         obligatoire).  -->
										<xsd:element name="LOYER" type="loyer_dets" />
										<!-- Dépôt de garantie.  -->
										<xsd:element name="DEPOT_GARANTIE" type="xsd:decimal"/>
										<!-- -->
										<xsd:element name="FRAIS_DIVERS" type="xsd:decimal" minOccurs="0"/>
										<!-- Quelle personne établit l’état des lieux. (voir dictionnaire des références)  -->
										<xsd:element name="ETAT_LIEUX" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="FRAIS_AGENCE" type="xsd:decimal" minOccurs="0"/>
										<!-- Honoraires pour la rédaction du bail à la charge du locataire.  -->
										<xsd:element name="HONORAIRES_BAIL_LOCATAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- Honoraires pour la rédaction du bail à la charge du propriétaire.  -->
										<xsd:element name="HONORAIRES_BAIL_PROPRIETAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- -->
										<xsd:element name="HONORAIRES_LOCATION_LOCATAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- -->
										<xsd:element name="HONORAIRES_LOCATION_PROPRIETAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- -->
										<xsd:element name="HONORAIRES_ETAT_LOCATAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- -->
										<xsd:element name="HONORAIRES_ETAT_PROPRIETAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- Honoraires pour la négociation / entremise à la charge du propriétaire.   -->
										<xsd:element name="HONORAIRES_NEGOCIATION_ENTREMISE" type="xsd:decimal" minOccurs="0"/>
										<!-- Honoraires pour la constitution du dossier à la charge du locataire.   -->
										<xsd:element name="HONORAIRES_FRAIS_DOSSIER_LOCATAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- Honoraires pour la constitution du dossier à la charge du propriétaire.   -->
										<xsd:element name="HONORAIRES_FRAIS_DOSSIER_PROPRIETAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- Honoraires pour la visite à la charge du locataire.   -->
										<xsd:element name="HONORAIRES_VISITE_LOCATAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- Honoraires pour la visite à la charge du propriétaire.   -->
										<xsd:element name="HONORAIRES_VISITE_PROPRIETAIRE" type="xsd:decimal" minOccurs="0"/>
										<!-- Total des honoraires listés ci-dessus à la charge du locataire.   -->
										<xsd:element name="HONORAIRES_TOTAL_LOCATAIRE" type="xsd:decimal"/>
										<!-- Total des honoraires listés ci-dessus à la charge du propriétaire.   -->
										<xsd:element name="HONORAIRES_TOTAL_PROPRIETAIRE" type="xsd:decimal"/>
										<!-- Existe-t-il une caution ?   -->
										<xsd:element name="CAUTION" type="BoolType" minOccurs="0"/>
										<!-- -->										
										<xsd:element name="PROVISION_SUR_CHARGES" type="xsd:decimal"/>	
										<!-- -->									
										<xsd:element name="CHARGES" type="xsd:decimal" minOccurs="0"/>
										<!-- Durée en année du bail.   -->
										<xsd:element name="DUREE_BAIL" type="xsd:int" minOccurs="0"/>										
										<!-- Date de libération du bien.   -->
										<xsd:element name="LIBRE_LE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="TYPE_MANDAT">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:length value="1" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="NUM_MANDAT" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="DATE_MANDAT" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Date du passage sous compromis.  -->
										<xsd:element name="DATE_COMPROMIS" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- 35 Locataire 36 Propriétaire  -->
										<xsd:element name="OCCUPE_PAR" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="LOYER_ACTUEL" type="xsd:decimal" minOccurs="0"/>
										<!-- -->
										<xsd:element name="DATE_FIN_BAIL" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Telephone Taille maximum : 15 caractères. -->
										<xsd:element name="TELEPHONE_LOC_ACTUEL" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations concernant la vente du bien. Elle peut être couplée avec la balise LOCATION
                                 dans le cas d’une vente location. Elle doit être contenue dans une balise BIEN  -->
							<xsd:element name="VENTE" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- Estimation du prix du bien.  -->
										<xsd:element name="ESTIMATION" type="xsd:decimal" minOccurs="0"/>
										<!-- Prix de vente du bien, H.A.I.  -->
										<xsd:element name="PRIX" type="xsd:decimal"/>
										<!-- Prix de vente du bien (net vendeur)  -->
										<xsd:element name="PRIX_HORS_HA" type="xsd:decimal" minOccurs="0"/>
										<!-- Honoraires de l’agence.  -->
										<xsd:element name="HONORAIRES" type="xsd:decimal" minOccurs="0"/>
										<!-- Numéro du mandat. Taille maximum : 15 caractères.  -->
										<xsd:element name="NUM_MANDAT" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Le type du mandat. S=simple, E= Exclusif, P=Privilégié. (voir dictionnaire des références). Taille maximum : 1 caractère.  -->
										<xsd:element name="TYPE_MANDAT">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="1" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Date du mandat  -->
										<xsd:element name="DATE_MANDAT" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="PASSE_SOUS_COMPROMIS" type="BoolType" minOccurs="0"/>
										<!-- Date du passage sous compromis.  -->
										<xsd:element name="DATE_COMPROMIS" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="TAXE_HABITATION" type="xsd:decimal" minOccurs="0"/>
										<!-- -->
										<xsd:element name="TAXE_FONCIERE" type="xsd:decimal" minOccurs="0"/>
										<!-- -->
										<xsd:element name="CHARGES_MENSUELLES" type="xsd:decimal" minOccurs="0"/>
										<!-- 37 Droit d'enregistrement 38 TVA 39 Cession de parts 40 Transfert d'actions -->
										<xsd:element name="REGIME_FISCAL" type="xsd:int" minOccurs="0"/>
										<!-- A qui revient la charge des honoraires. 0=vendeur, 1=acquéreur  -->
										<xsd:element name="CHARGES_HONORAIRES" type="BoolType" minOccurs="0"/>
										<!-- v2.3 -->
										<!-- % des honoraires TTC à la charge de l’acquéreur par rapport au prix de vente  -->
										<xsd:element name="HONORAIRES_POURCENTAGE_ACQUEREUR" type="xsd:decimal" minOccurs="0"/>
										<!-- v2.3 -->
										<!-- Quote-part budget prévisionnel dépenses courantes charge du vendeur (€/an)  -->
										<xsd:element name="COPRO_QUOTE_PART" type="xsd:decimal" minOccurs="0"/>
										<!-- v2.3 -->
										<!-- Nombre de lots dans la copropriété  -->
										<xsd:element name="COPRO_NOMBRE_LOTS" type="xsd:int" minOccurs="0"/>
										<!-- v2.3 -->
										<!-- Numéro du lot  -->
										<xsd:element name="COPRO_NUM_LOT" type="xsd:string" minOccurs="0"/>
										<!-- v2.3 -->
										<!-- Procédures diligentées contre le syndicat de copropriété (0 = non, 1 = oui)  -->
										<xsd:element name="COPRO_PROC_DILIG" type="xsd:int" minOccurs="0"/>
										<!-- v2.3 -->
										<!-- Demande de désignation d’un mandataire ad hoc (0 = non, 1 = oui, autre = non spécifié) -->
										<xsd:element name="COPRO_MAN_ADHOC" type="xsd:int" minOccurs="0"/>
										<!-- v2.3 -->
										<!-- Demande de désignation d’un administrateur provisoire (0 = non, 1 = oui, autre = non spécifié)  -->
										<xsd:element name="COPRO_ADMIN_PROV" type="xsd:int" minOccurs="0"/>
										<!-- v2.3 -->
										<!-- Demande de désignation d’experts (0 = non, 1 = oui, autre = non spécifié)  -->
										<xsd:element name="COPRO_DES_EXP" type="xsd:int" minOccurs="0"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations concernant le viager. Elle apparaît seule et ne peut être couplée avec une balise de transaction. 
                             Elle peut être contenue dans une balise BIEN  -->
							<xsd:element name="VIAGER" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- Nombre de têtes sur laquelle porte la rente.  -->
										<xsd:element name="NOMBRE_TETES" type="xsd:int" minOccurs="0"/>
										<!-- Montant du bouquet.  -->
										<xsd:element name="BOUQUET" type="xsd:decimal"/>
										<!-- Montant de la rente.  -->
										<xsd:element name="RENTE" type="xsd:decimal" minOccurs="0"/>
										<!-- Date du mandat.  -->
										<xsd:element name="DATE_MANDAT" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Numéro du mandat. Taille maximum : 15 caractères.  -->
										<xsd:element name="NUM_MANDAT" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="15" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Le type du mandat. S=simple, E= Exclusif, P=Privilégié. Taille maximum : 1 caractère.  -->
										<xsd:element name="TYPE_MANDAT">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="1" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Date du passage sous compromis -->
										<xsd:element name="DATE_COMPROMIS" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations génériques du bien maison. Cette balise de bien ne peut être couplée à une autre 
                                 Elle peut être contenue dans une balise BIEN  -->
							<xsd:element name="MAISON" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- Nombre de pièces.  -->
										<xsd:element name="NBRE_PIECES" type="xsd:int"/>
										<!-- Nombre de chambres.  -->
										<xsd:element name="NBRE_CHAMBRES" type="xsd:int"/>
										<!-- Surface totale. --> 
										<xsd:element name="SURFACE_HABITABLE" type="xsd:int"/>
										<!-- Surface séjour.  -->
										<xsd:element name="SURFACE_SEJOUR" type="xsd:int"/>
										<!-- Surface terrain.  -->
										<xsd:element name="SURFACE_TERRAIN" type="xsd:int"/>
										<!-- Nombre de niveaux.  -->
										<xsd:element name="NBRE_NIVEAUX" type="xsd:int"/>
										<!-- Année de construction du bien.  -->
										<xsd:element name="ANNEE_CONSTRUCTION" type="xsd:int"/>
										<!-- Surface des dépendances.   -->
										<xsd:element name="SURFACE_DEPENDANCE" type="xsd:int" minOccurs="0"/>
										<!--  -->
										<xsd:element name="CATEGORIE" type="categorie_maison"/>
										<!-- -->
										<xsd:element name="ETAT_GENERAL" type="etat_general_type" minOccurs="0"/>
										<!-- Etat intérieur. 71 Somptueux 72 A rafraîchir 73 Très bon 74 Bon 75 Etat moyen 76 Habitable en l'état 77 Travaux à prévoir   
										 -->
										<xsd:element name="ETAT_INTERIEUR" type="etat_interieur_type" minOccurs="0"/>
										<!-- Etat extérieur. 81 Somptueux 82 Très bon 83 Bon 84 Travaux à prévoir 85 A réhabiliter 86 A rafraîchir -->
										<xsd:element name="ETAT_EXTERIEUR" type="etat_exterieur_type" minOccurs="0"/>
										<!-- Nombre de salles de bains.   -->
										<xsd:element name="NBRE_SALLE_BAIN" type="xsd:int"/>
										<!-- Nombre de garages.   -->
										<xsd:element name="NBRE_GARAGE" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de balcons.   -->
										<xsd:element name="NBRE_BALCON" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de salles d’eau.   -->
										<xsd:element name="NBRE_SALLE_EAU" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de parkings.   -->
										<xsd:element name="NBRE_PARKING" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de terrasse  -->
										<xsd:element name="NBRE_TERRASSE" type="xsd:int" minOccurs="0"/>
										<!-- 18 Aménagée 19 Equipée 20 Kitchenette 337 Simple 359 sans cuisine 402 Américaine 403 Nue  -->
										<xsd:element name="CUISINE" type="xsd:int" minOccurs="0"/>
										<!-- Le bien possède-t-il un sous-sol ?  -->
										<xsd:element name="SOUS_SOL" type="BoolType" minOccurs="0"/>
										<!-- 41 Pierre 42 Brique 43 Parpaing 44 Béton 45 Bois 46 Meulière -->
										<xsd:element name="CONSTRUCTION" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="MITOYENNETE" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="STANDING" type="xsd:int" minOccurs="0"/>
										<!-- Le bien est ‘meublé’ ou non.  -->
										<xsd:element name="MEUBLE" type="BoolType"/>
										<!-- 48 Terrasse 49 Tuiles 50 Ardoises 51 Chaumes 52 Autre 53 Tôle 429 Shingle  -->
										<xsd:element name="COUVERTURE" type="xsd:int" minOccurs="0"/>
										<!-- contient les informations de situation du bien -->
										<xsd:element name="LOCALISATION" type="xsd:int" minOccurs="0"/>
										<!--  -->
										<xsd:element name="LOTISSEMENT" type="BoolType" minOccurs="0"/>
										<!-- 7 Est 8 Nord 9 Nord-est 10 Nord-ouest 11 Ouest 12 Sud 13 Sud-ouest 14 Sud-est 404 Est-Ouest 405 Nord-Sud 414 Ouest-Est --> 
										<xsd:element name="EXPOSITION" type="xsd:int" minOccurs="0"/>
										<!-- 21 Collectif  22 Individuel 23 Central 24 Oui sans précision 25 Pompe a chaleur 26 Climatisation 406 Sans climatisation 
                                             423 Aucun 425 Production Solaire 426 Production Géothermique 444 Mixte  -->
										<xsd:element name="CHAUFFAGE" type="xsd:int" minOccurs="0"/>
										<!-- 31 Gaz 32 Electrique 33 Fuel 34 Autres 415 Gaz ville 416 Gaz citerne 417 Charbon 418 Bois 419 Solaire 422 Géothermique 
                                             451 Sans -->
										<xsd:element name="MECANISME_CHAUFFAGE" type="xsd:int" minOccurs="0"/>
										<!-- 27 Radiateur 28 Au sol 29 Air pulsé 30 Convecteurs 430 Panneau rayonnant  -->
										<xsd:element name="MODE_CHAUFFAGE" type="xsd:int" minOccurs="0"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations génériques du bien appartement. Cette balise de bien peut être couplée à une balise <RESIDENCE>. 
                                 Elle peut être contenue dans une balise BIEN  -->
							<xsd:element name="APPARTEMENT" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- Numéro de l’étage.  -->
										<xsd:element name="NUM_ETAGE" type="xsd:int" minOccurs="0"/>
										<!-- Numéro du dernier étage.  -->
										<xsd:element name="NUM_DERNIER_ETAGE" type="xsd:int" minOccurs="0"/>
										<!-- Surface totale.  -->
										<xsd:element name="SURFACE_HABITABLE" type="xsd:int"/>										
										<!-- Surface séjour.  -->
										<xsd:element name="SURFACE_SEJOUR" type="xsd:int"/>
										<!-- Nombre de pièces.  -->
										<xsd:element name="NBRE_PIECES" type="xsd:int"/>
										<!-- Nombre de chambres.  -->
										<xsd:element name="NBRE_CHAMBRES" type="xsd:int"/>
										<!-- Année de construction du bien -->
										<xsd:element name="ANNEE_CONSTRUCTION" type="xsd:int"/>
										<!--  -->
										<xsd:element name="TERRAIN_PRIVATIF" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de niveau du bien.  -->
										<xsd:element name="NBRE_NIVEAUX" type="xsd:int"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CATEGORIE" type="categorie_appartement"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="ETAT_GENERAL" type="etat_general_type" minOccurs="0"/>
										<!-- Nombre de salles de bain du bien.  -->
										<xsd:element name="NBRE_SALLE_BAIN" type="xsd:int"/>
										<!-- Nombre de garages.  -->
										<xsd:element name="NBRE_GARAGE" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de balcons.  -->
										<xsd:element name="NBRE_BALCON" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de salles d’eau.  -->
										<xsd:element name="NBRE_SALLE_EAU" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de parkings.  -->
										<xsd:element name="NBRE_PARKING" type="xsd:int" minOccurs="0"/>
										<!-- NOMBRE DE TERRASSE -->
										<xsd:element name="NBRE_TERRASSE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CUISINE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="EAU_CHAUDE" type="xsd:int" minOccurs="0"/>
										<!-- L’immeuble possède un accès handicapés ou non.  -->
										<xsd:element name="ACCES_HANDICAPES" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="ASCENSEUR" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="STANDING" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="MEUBLE" type="BoolType" />
										<!-- -->
										<xsd:element name="NBRE_CAVES" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="LOCALISATION" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="EXPOSITION" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="CHAUFFAGE" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="MECANISME_CHAUFFAGE" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="MODE_CHAUFFAGE" type="xsd:int" minOccurs="0"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations génériques du bien terrain. Cette balise de bien ne peut être couplée à une autre 
                                 Elle peut être contenue dans une balise BIEN  -->
							<xsd:element name="TERRAIN" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- Coefficient d’occupation des Sols.  -->
										<xsd:element name="COS" type="xsd:decimal" minOccurs="0"/>
										<!-- Surface Hors Oeuvre Nette.  -->
										<xsd:element name="SHON" type="xsd:int" minOccurs="0"/>										
										<!-- Surface du Terrain.  -->
										<xsd:element name="SURFACE_TERRAIN" type="xsd:int"/>
										<!-- -->
										<xsd:element name="DATE_CA" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Date de remise du certificat d'urbanisme.  -->
										<xsd:element name="DATE_CU" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Hauteur maximum du terrain.  -->
										<xsd:element name="HAUTEUR_MAXI" type="xsd:int" minOccurs="0"/>
										<!--  -->
										<xsd:element name="LONGUEUR_FACADE" type="xsd:int" minOccurs="0"/>
										<!-- Date du permis de construire.  -->
										<xsd:element name="DATE_PC" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="VIABILITE" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="GAZ" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="LOTISSEMENT" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="ASSAINISSEMENT" type="BoolType" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="URBANISME" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="LOCALISATION" type="xsd:int" minOccurs="0"/>
										<!--  -->
										<xsd:element name="CLOTURE" type="BoolType" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="ASPECT" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CATEGORIE" type="categorie_terrain"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations spécifiques à la résidence de l’appartement. Cette balise doit être couplée à une balise <APPARTEMENT>. Elle peut être contenue dans une balise BIEN  -->

							<xsd:element name="RESIDENCE" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- Non de la résidence.  -->
										<xsd:element name="RESIDENCE_NOM" type="xsd:string" minOccurs="0"/>
										<!-- Nombre d’étages de la résidence.  -->
										<xsd:element name="NB_ETAGES" type="xsd:int" minOccurs="0"/>
										<!-- Année ou époque de construction du bien. Taille maximum : 15 caractères.  -->
										<xsd:element name="ANNEE_CONSTRUCTION" type="xsd:string"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CONSTRUCTION" type="xsd:int"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="TYPE_CHAUFFAGE" type="xsd:int"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="EAU_CHAUDE" type="xsd:int"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="STANDING" type="xsd:int"/>
										<!-- La résidence possède-t-elle un tennis ?  -->
										<xsd:element name="TENNIS" type="BoolType"/>
										<!-- La résidence possède-t-elle un jardin ?  -->
										<xsd:element name="JARDIN" type="BoolType"/>
										<!-- La résidence a-t-elle un gardien ?  -->
										<xsd:element name="GARDIEN" type="BoolType"/>
										<!-- La résidence possède-t-elle un ascenseur ?  -->
										<xsd:element name="ASCENSEUR" type="BoolType"/>
										<!-- La résidence possède-t-elle une piscine ?  -->
										<xsd:element name="PISCINE" type="BoolType"/>
										<!-- La résidence possède-t-elle un parc ?  -->
										<xsd:element name="PARC" type="BoolType"/>
										<!-- Nom de la localité où se trouve la résidence -->
										<xsd:element name="NOM_LOCALITE" type="xsd:string" minOccurs="0"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations génériques du bien demeure. Cette balise de bien ne peut être couplée à une autre 
                                 Elle peut être contenue dans une balise  -->
							<xsd:element name="DEMEURE" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- -->
										<xsd:element name="NBRE_PIECES" type="xsd:int"/>
										<!-- -->
										<xsd:element name="NBRE_CHAMBRES" type="xsd:int"/>
										<!-- -->
										<xsd:element name="SURFACE_HABITABLE" type="xsd:int"/>
										<!-- -->
										<xsd:element name="SURFACE_SEJOUR" type="xsd:int"/>
										<!-- -->
										<xsd:element name="ETAT_INTERIEUR" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="ETAT_EXTERIEUR" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="SURFACE_DEPENDANCE" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="SURFACE_PARC" type="xsd:int" minOccurs="0"/>
										<!-- Description de l’époque du bien. Taille maximum : 30 caractères.  -->
										<xsd:element name="EPOQUE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="30" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Surface autres terrains.  -->
										<xsd:element name="SURFACE_AUTRE_TERRAIN" type="xsd:int" minOccurs="0"/>
										<!-- Surface des parties boisées du bien.  -->
										<xsd:element name="SURFACE_BOIS" type="xsd:int" minOccurs="0"/>
										<!-- Surface Terre.  -->
										<xsd:element name="SURFACE_TERRE" type="xsd:int" minOccurs="0"/>
										<!-- Surface de l’étang, si il y a lieu.  -->										
										<xsd:element name="SURFACE_ETANG" type="xsd:int" minOccurs="0"/>
										<!-- Longueur de larivière, si il y a lieu.  -->
										<xsd:element name="LONGUEUR_RIVIERE" type="xsd:int" minOccurs="0"/>										
										<!-- Distance du bien par rapport à une gare.  -->
										<xsd:element name="DISTANCE_TRAIN" type="xsd:int" minOccurs="0"/>
										<!-- Distance du bien par rapport à un aéroport.  -->
										<xsd:element name="DISTANCE_AEROPORT" type="xsd:int" minOccurs="0"/>
										<!-- Distance du bien par rapport à une autoroute.  -->
										<xsd:element name="DISTANCE_AUTOROUTE" type="xsd:int" minOccurs="0"/>
										<!-- Distance du bien par rapport aux différents types de commodités.  -->
										<xsd:element name="DISTANCE_COMMODITES" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CATEGORIE" type="categorie_demeure"/>
										<!-- Nombre de salles de bain.  -->
										<xsd:element name="NBRE_SALLE_BAIN" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de garages.  -->
										<xsd:element name="NBRE_GARAGE" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de niveaux.  -->
										<xsd:element name="NBRE_NIVEAU" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de balcons.  -->
										<xsd:element name="NBRE_BALCON" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de salle d’eau.  -->
										<xsd:element name="NBRE_SALLE_EAU" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de parkings.  -->
										<xsd:element name="NBRE_PARKING" type="xsd:int" minOccurs="0"/>
										<!-- Nombre de terrasses.  -->
										<xsd:element name="NBRE_TERRASSE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CUISINE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="EAU_CHAUDE" type="xsd:int" minOccurs="0"/>
										<!-- Le bien possède un sous-sol ou non.  -->
										<xsd:element name="SOUS_SOL" type="BoolType" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CONSTRUCTION" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="MITOYENNETE" type="xsd:int" minOccurs="0"/>
										<xsd:element name="ISOLATION" type="BoolType" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="COUVERTURE" type="xsd:int" minOccurs="0"/>
										<xsd:element name="NBRE_CAVES" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="LOCALISATION" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CLASSEMENT" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CHAUFFAGE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="MECANISME_CHAUFFAGE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->										
										<xsd:element name="MODE_CHAUFFAGE" type="xsd:int" minOccurs="0"/>
										<!-- Le bien possède-t-il une piscine ?  -->
