																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:maxLength value="30" fixed="true"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<!-- Obligatoire dans la balise <ALENTOURS> -->
																<xsd:element name="DISTANCE" type="xsd:int"/>
															<!-- Obligatoire dans la balise <ALENTOURS>. Distance exprimée en Km ou m. Taille maximum : 5 caractères. -->
																<xsd:element name="UNITE">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:maxLength value="5" fixed="true"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
									        </xsd:element>
										<!-- Une activité de ski de fond est-elle possible ? -->
										<xsd:element name="ACTIVITES_SKI_FOND" type="BoolType" minOccurs="0"/>
										<!-- Une activité de ski alpin est-elle possible ? -->
										<xsd:element name="ACTIVITES_SKI_ALPIN" type="BoolType" minOccurs="0"/>
										<!-- Une activité golf est-elle possible ? Description du format des fichiers de transfert des biens immobiliers -->
										<xsd:element name="ACTIVITES_GOLF" type="BoolType" minOccurs="0"/>
										<!-- Une activité VTT est-elle possible ? -->
										<xsd:element name="ACTIVITES_VTT" type="BoolType" minOccurs="0"/>
										<!-- Une activité équitation est-elle possible ? -->
										<xsd:element name="ACTIVITES_EQUITATION" type="BoolType" minOccurs="0"/>
										<!-- Une activité randonnée est-elle possible ? -->
										<xsd:element name="ACTIVITES_RANDONNEE" type="BoolType" minOccurs="0"/>
										<!-- Une activité parapente est-elle possible ? -->
										<xsd:element name="ACTIVITES_PARAPENTE" type="BoolType" minOccurs="0"/>
										<!-- Une activité de sports nautiques est-elle possible ? -->
										<xsd:element name="ACTIVITES_SPORTS_NAUTIQUE" type="BoolType" minOccurs="0"/>
										<!-- Ligne 1 de l'adresse. Taille maximum : 40 caractères.  -->
										<!-- Les locations de vacances sont un cas particulier, la localisation est intégrée dans sa définition. Taille maximum : 40 
                                             caractères. -->
										<xsd:element name="ADRESSE1">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Ligne 2 de l'adresse. Taille maximum : 40 caractères.  -->
										<xsd:element name="ADRESSE2" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Ligne 3 de l'adresse. Taille maximum : 40 caractères.  -->
										<xsd:element name="ADRESSE3" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Ligne 4 de l'adresse. Taille maximum : 40 caractères.  -->
										<xsd:element name="ADRESSE4" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Code postal de la commune. Taille maximum : 5 chiffres -->
										<xsd:element name="CP">
											<xsd:simpleType>
												<xsd:restriction base="xsd:int">
													<xsd:pattern value="(\d{5})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Ville du propriétaire. Taille maximum : 50 caractères. -->
										<xsd:element name="VILLE">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Code du pays -->
										<xsd:element name="PAYS">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="20" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Texte libre pour préciser la localisation. Taille maximum : 40 caractères. -->
										<xsd:element name="LOCALISATION" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- (voir dictionnaire des références) -->
										<xsd:element name="CATEGORIE" type="categorie_vacances"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations d’occupation de la location vacances. Cette balise doit obligatoirement figurer avec la balise VACANCES. Elle n’apparaît qu’une fois. Elle peut être contenue dans une balise BIEN. Par défaut chaque jour de l’année est considéré comme ‘hors location’. Seules sont envoyées les périodes libres et les périodes occupées.  -->
							<xsd:element name="PLANNING" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Module de balise contenant trois éléments enfants. Balises incluses dans OCCUPATION  -->
										<xsd:element name="OCCUPATION" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<!-- -->
													<xsd:element name="DATE_DEBUT" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
													<!-- -->
													<xsd:element name="DATE_FIN" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
													<!-- Appartient au module de balise OCCUPATION. Si false : Libre.   -->
													<xsd:element name="OCCUPE" type="BoolType" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations de tarifs de la location vacances. Cette balise doit obligatoirement figurer avec la balise <VACANCES>. Elle n’apparaît qu’une fois. Elle peut être contenue dans une balise BIEN  -->
							<xsd:element name="TARIFICATION" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- -->
										<xsd:element name="TARIF" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<!-- -->
													<xsd:element name="DATE_DEBUT" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
													<!-- -->
													<xsd:element name="DATE_FIN" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
													<!-- -->
													<xsd:element name="PRIX" type="xsd:decimal" minOccurs="0"/>
													<!-- Appartient au module de balise TARIF. Correspond au nombre de jours.  -->
													<xsd:element name="TYPE_TARIFICATION" type="xsd:int" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations confidentielles du bien. Cette balise accompagne une balise de bien. Elle n’apparaît qu’une fois. 
                             Elle peut être contenue dans une balise BIEN   -->
							<xsd:element name="CONFIDENTIEL" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- -->
										<xsd:element name="SYNDIC" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="NOTAIRE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Description du moyen de visite. Taille maximum : 50 caractères. -->
										<xsd:element name="MOYEN_VISITE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="CLES_AGENCE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="COPROPRIETE" type="xsd:int" minOccurs="0"/>
										<!-- Seulement dans les locaux. Pour le local commercial, cette balise correspond au nom d’immeuble. Taille maximum : 50  -->
										<xsd:element name="NOM_SITE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="50" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!--  Cette balise contient les informations de situation du bien. Cette balise accompagne une balise de bien. Elle n’apparaît qu’une fois. 
                                  Elle peut être contenue dans une balise BIEN  -->
							<xsd:element name="LOCALISATION">
								<xsd:complexType>
									<xsd:all>
										<!-- Code postal de la commune. Taille maximum : 5 chiffres. -->
										<xsd:element name="CODE_POSTAL">
											<xsd:simpleType>
												<xsd:restriction base="xsd:int">
													<xsd:pattern value="(\d{5})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Code INSEE de la commune. Ce champ n'est pas obligatoire aujourd'hui mais le sera plus tard. Taille maximum : 5 chiffres. -->
										<xsd:element name="CODE_INSEE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:int">
													<xsd:pattern value="(\d{5})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Ville du propriétaire. Taille maximum : 50 caractères. -->
										<xsd:element name="VILLE" type="xsd:string" minOccurs="0"/>
										<!-- -->
										<xsd:element name="VILLE_INTERNET" type="xsd:string" minOccurs="0"/>
										<!-- -->
										<xsd:element name="CP_INTERNET" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:int">
													<xsd:pattern value="(\d{5})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="NUM_RUE" type="xsd:int" minOccurs="0"/>
										<!-- Le numéro est-il un ‘bis’ ?  -->
										<xsd:element name="BIS_RUE" type="BoolType" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="TYPE_RUE" type="xsd:int" minOccurs="0"/>
										<!-- Numéro de la rue.  -->
										<xsd:element name="NOM_RUE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Nom de la résidence. Taille maximum : 80 caractères.  -->
										<xsd:element name="RESIDENCE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="80" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Nom du quartier .Taille maximum : 80 caractères.  -->
										<xsd:element name="QUARTIER" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="80" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Texte libre indiquant la situation du bien par exemple : ’centre ville’. Taille maximum : 80 caractères.  -->
										<xsd:element name="SITUATION">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="80" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Visibilité du code postal sur internet.  -->
										<xsd:element name="VISIBLE" type="BoolType"/>
										<!-- Longitude internet. -->
										<xsd:element name="LONGITUDE" type="xsd:decimal" minOccurs="0" />
										<!-- Latitude internet. -->
										<xsd:element name="LATITUDE" type="xsd:decimal" minOccurs="0" />
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- commentaires Taille maximum : 255 caractères -->
							<xsd:element name="COMMENTAIRES">
								<xsd:complexType>
									<xsd:all>
										<!-- Commentaires en langue française. Taille maximum : 1200 caractères.   -->
										<xsd:element name="FR" minOccurs="0" >
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="1200" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Commentaires en langue anglaise. Taille maximum : 1200 caractères.   -->
										<xsd:element name="US" minOccurs="0" >
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="1200" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Commentaires en allemand. Taille maximum : 1200 caractères.   -->
										<xsd:element name="DE" minOccurs="0" >
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="1200" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Commentaires en espagnol. Taille maximum : 1200 caractères.   -->
										<xsd:element name="ES" minOccurs="0" >
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="1200" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Commentaires en italien. Taille maximum : 1200 caractères.   -->
										<xsd:element name="IT" minOccurs="0" >
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="1200" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Commentaires destinés à l’inter-cabinet, n’apparaît pas sur les sites FNAIM. Taille maximum : 4000 caractères.   -->
										<xsd:element name="LIBRE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="4000" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Commentaires confidentiels destinés aux SIA. Taille maximum : 1200 caractères. -->
										<xsd:element name="INFO_CONFIDENTIELS" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="1200" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="VISITE_VIRTUELLE" type="xsd:string" minOccurs="0"/>
										<!-- -->
										<xsd:element name="ACCROCHE_UN" type="xsd:string" minOccurs="0"/>
										<!-- -->
										<xsd:element name="ACCROCHE_DEUX" type="xsd:string" minOccurs="0"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les images associées au bien. Cette balise accompagne une balise de bien. Elle n’apparaît qu’une fois. 
                                 Elle peut être contenue dans une balise BIEN  -->
							<xsd:element name="IMAGES" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="IMG" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<!-- Cette balise doit être créée impérativement à vide. Elle est nécessaire pour la gestion des flux vers les 
													serveurs FNAIM -->
													<xsd:element name="DATA" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
												<!-- Attribut qui indique que si l’on doit supprimer cette image ou pas.  -->
												<xsd:attribute name="supprimerimg" type="BoolType" use="optional"/>
												<!-- Numéro de l’image. Ce numéro doit être compris entre 1 et 10 inclus.  -->
												<xsd:attribute name="num" type="xsd:int"/>
												<!-- Chemin et nom du fichier (ne pas mettre l’extension du fichier ‘.jpg’). Le chemin peut être absolu ou relatif. Taille 
												maximum : 150 caractères.  -->                                                                                             
												<xsd:attribute name="nom">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:maxLength value="150"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<!-- Légende de l’image. Taille maximum : 100 caractères.  -->
												<xsd:attribute name="commentaire">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:maxLength value="100"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
                         	<!-- Cette balise contient les informations nécessaires pour archiver l’affaire. Elle n’apparaît que si l’attribut "action" est égal à "S", une seule fois. Elle doit être contenue dans une balise <BIEN>.  -->						
							<xsd:element name="ARCHIVAGE" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- 1 Vendue par l'agence 2 Retirée de la vente 3 Fin de mandat 4 Vendue par un confrère 5 Vendue par le propriétaire 
                                             6 Vendue par autre intermédiaire 7 Vendue aux enchères 8 loué Description du format des fichiers de transfert des biens immob
                                             9 non loué -->
										<xsd:element name="MOTIF_SUPPRESSION" type="xsd:integer"/>
										<!-- Prix de vente ou loyer lors de la transaction.  -->
										<xsd:element name="PRIX_VENTE" type="xsd:decimal"/>
										<!-- Module de balise contenant sept éléments enfants.  -->
										<xsd:element name="ACQUEREUR" minOccurs="0">
											<xsd:complexType>
												<xsd:all>
                                                    <!-- Nom du négociateur. Taille maximum : 40 caractères. -->
													<xsd:element name="NOM" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:maxLength value="40" fixed="true"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
                                                    <!-- Prénom du négociateur. Taille maximum : 40 caractères. -->
													<xsd:element name="PRENOM" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:maxLength value="40" fixed="true"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
													<xsd:element name="ADRESSE" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:maxLength value="40" fixed="true"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
													<!-- Code postal de la commune. Taille maximum : 5 chiffres -->
													<xsd:element name="CP" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:pattern value="(\d{5})"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
													<!-- Ville du propriétaire. Taille maximum : 50 caractères. -->
													<xsd:element name="VILLE" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:maxLength value="50" fixed="true"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
													<!-- Code du pays -->
													<xsd:element name="PAYS" type="xsd:integer" minOccurs="0"/>
													<!-- Appartient au module de balise <ACQUEREUR>. Taille maximum : 50 caractères.  -->
													<xsd:element name="PROFESSION" minOccurs="0">
														<xsd:simpleType>
															<xsd:restriction base="xsd:string">
																<xsd:maxLength value="50" fixed="true"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:element>
												</xsd:all>
											</xsd:complexType>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>

						</xsd:all>
						<xsd:attribute name="action" type="xsd:string" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<!-- Cette balise décrit un bien qui a été archivé ou un bien à supprimer.-->
				<xsd:element name="SUPPRESSION" minOccurs="0">
				    <xsd:complexType>
				      <xsd:sequence>
				        <xsd:element maxOccurs="unbounded" name="BIEN_SUPP">
				          <xsd:complexType>
				            <xsd:sequence>
                                <!-- Numéro FNAIM de l'agence. Un numéro d'adhérent FNAIM est compris entre 1 et 599 999  -->
								<xsd:element name="ADH_NUM" type="xsd:int" />
								<!-- v2.3 -->
								<!--
								  Le numéro SIREN dans le cas d’un agence principale / Le numéro SIRET dans le cas d’une succursale. 
								  Cet identifiant nous permet de faire le rapprochement entre les informations des agences reçues et nos adhérents. 
								  Tout mandat appartenant à une agence non identifiable par le numéro SIRET ou SIREN transmis ou non sera rejeté. -->
								<xsd:element name="ID_ENTREPRISE" type="xsd:string" minOccurs="0"/>
								<!-- Numéro de l’affaire. Taille maximum : 50 caractères. -->
								<xsd:element name="AFF_NUM">
				              		<xsd:simpleType>
									<xsd:restriction base="xsd:string">
									<xsd:maxLength value="50" fixed="true"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							  <!-- Date de la suppression de l’affaire, correspond à la date de vente ou de location de l’affaire. -->
				              <xsd:element name="DATE_SUPPRESSION" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						      <!-- Champ indiquant le type de bien suivant la normalisation Iris. Ex : "Appartement", "Demeure","Terrain","Parking", etc. -->
				              <xsd:element name="TYPE_BIEN" type="xsd:string"  minOccurs="0"/>
				              <!-- Champ indiquant la transaction suivant la normalisation Iris. Ex : "Vente", "Location","Viager". -->
				              <xsd:element name="TRANSACTION" type="xsd:string"  minOccurs="0"/>
				            </xsd:sequence>
				          </xsd:complexType>
				        </xsd:element>
				      </xsd:sequence>
				    </xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<!-- -->
			<xsd:attribute name="origine" type="xsd:string" use="required"/>
			<!-- 1.0 -->
			<xsd:attribute name="version" type="xsd:decimal" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="loyer_dets" mixed="true">
		<xsd:all>
			<!-- Loyer mensuel hors charges -->
			<xsd:element name="HORS_CHARGES" type="xsd:decimal" minOccurs="0"/>
			<!-- Loyer mensuel charges comprises -->
			<xsd:element name="CHARGES_COMPRISES" type="xsd:decimal" minOccurs="0"/>
			<!-- Loyer au m² par mois. -->
			<xsd:element name="METRE_CARRE_MENSUEL" type="xsd:decimal" minOccurs="0"/>
			<!-- Loyer au m² par an. -->
			<xsd:element name="METRE_CARRE_ANNUEL" type="xsd:decimal" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
	<!--  -->
	<xsd:simpleType name="BoolType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="True"/>
			<xsd:enumeration value="False"/>
			<xsd:enumeration value="true"/>
			<xsd:enumeration value="false"/>
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--  -->
	<xsd:simpleType name="etat_general_type">
		<xsd:restriction base="xsd:int">
			<!-- non défini -->
			<xsd:enumeration value="-1"/>
			<xsd:enumeration value="0"/>
			<!-- Habitable -->
			<xsd:enumeration value="68"/>
			<!-- Travaux à prévoir -->
			<xsd:enumeration value="69"/>
			<!-- Très bon état -->
			<xsd:enumeration value="70"/>
			<!-- Etat moyen -->
			<xsd:enumeration value="411"/>
			<!-- Neuf -->
			<xsd:enumeration value="412"/>
			<!-- Bon état -->
			<xsd:enumeration value="431"/>
