										<xsd:element name="NBRE_LOTS" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="GROS_OEUVRE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="ANCIENNETE" type="xsd:int" minOccurs="0"/>
										<!--(voir dictionnaire des références)  -->
										<xsd:element name="ZONE_ECONOMIQUE" type="xsd:int" minOccurs="0"/>
										<!-- Surface utile du bien.  -->
										<xsd:element name="SURFACE_UTILE" type="xsd:int"/>
										<!-- Surface totale du bien.  -->
