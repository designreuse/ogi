										<xsd:element name="SURFACE_DIVISIBLE" type="BoolType" minOccurs="0"/>
										<!-- Surface minimum du bien.  -->
										<xsd:element name="SURFACE_MINIMUM" type="xsd:int"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CHAUFFAGE" type="xsd:int" minOccurs="0"/>										
                                        <!-- Téléphone du négociateur. Taille maximum : 15 caractères.  -->
										<xsd:element name="TELEPHONE" type="BoolType" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="TYPE_TELEPHONE" type="xsd:int" minOccurs="0"/>
										<!-- Le nombre de lignes téléphonique du bien.  -->
										<xsd:element name="NBRE_LIGNES" type="xsd:int" minOccurs="0"/>
										<xsd:element name="IMMEUBLE_COLLECTIF" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un monte-charge ?  -->
										<xsd:element name="MONTE_CHARGE" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un quai de déchargement ?  -->
										<xsd:element name="QUAI" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il une aire de manoeuvre?  -->
										<xsd:element name="AIRE_MANOEUVRE" type="BoolType" minOccurs="0"/>
										<xsd:element name="IMMEUBLE_INDIVIDUEL" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il une climatisation ?  -->
										<xsd:element name="CLIMATISATION" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il des sprinklers ?  -->
										<xsd:element name="SPRINKLERS" type="BoolType" minOccurs="0"/>
										<xsd:element name="AIR_COMPRIME" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un ascenseur ?  -->
										<xsd:element name="ASCENSEUR" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il une alarme incendie ?  -->
										<xsd:element name="RESEAU_ALARME_INCENDIE" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un pont roulant ?  -->
										<xsd:element name="PONT_ROULANT" type="BoolType" minOccurs="0"/>
										        <!-- Le bien est-il près d’un accès routier ?  -->
								                <xsd:element name="PROXIMITE_ACCES_ROUTIER" type="BoolType"  minOccurs="0"/>							                
								                <xsd:element name="PROXIMITE_ACCES_TOUT_TONNAGE" type="BoolType"  minOccurs="0"/>
								                <!-- Le bien est-il près d’une aérogare ?  -->
								                <xsd:element name="PROXIMITE_AEROGARE" type="BoolType"  minOccurs="0"/>
								                <!-- Le bien est-il près d’une gare routière ?  -->
								                <xsd:element name="PROXIMITE_GARE_ROUTIERE" type="BoolType"  minOccurs="0"/>								               
								                <xsd:element name="PROXIMITE_COMMERCES" type="BoolType"  minOccurs="0"/>
								                <!-- Le bien est-il près d’une gare de Métro ou RER ?  -->
								                <xsd:element name="PROXIMITE_METRO_RER" type="BoolType"  minOccurs="0"/>
								                <!-- Le bien est-il près d’une gare SNCF ?  -->
								                <xsd:element name="PROXIMITE_SNCF" type="BoolType"  minOccurs="0"/>
								                <!-- Le bien est-il près d’une voie fluviale ?  -->
								                <xsd:element name="PROXIMITE_VOIE_FLUVIALE" type="BoolType"  minOccurs="0"/>
								                <!-- Marché d’Intérêt Régional.  -->
								                <xsd:element name="MIR" type="BoolType"  minOccurs="0"/>
								                <!-- Marché d’Intérêt National. -->
								                <xsd:element name="MIN" type="BoolType"  minOccurs="0"/>
								        <!-- (voir dictionnaire des références)  -->        
										<xsd:element name="TOITURE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="SOLS" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="COUVERTURE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="STRUCTURE" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CHARPENTE" type="xsd:int" minOccurs="0"/>										
										<!-- Le bien possède-t-il un groupe électrogène ?  -->
										<xsd:element name="GROUPE_ELECTROGENE" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un onduleur ?  -->
										<xsd:element name="ONDULEUR" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il une ligne triphasé ?  -->
										<xsd:element name="TRIPHASE" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il une ligne monophasé ?  -->
										<xsd:element name="MONOPHASE" type="BoolType" minOccurs="0"/>
										<xsd:element name="PUISSANCE_EDF" type="xsd:int" minOccurs="0"/>
										<!-- (voir dictionnaire des références) -->
										<xsd:element name="CATEGORIE" type="categorie_local_industriel"/>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<!-- -->
							<xsd:element name="FOND_COMMERCE" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Peut apparaître 5 fois maximum. (voir dictionnaire des références)  -->
										<xsd:element name="ACTIVITE" type="xsd:int" maxOccurs="5"/>
										<!-- Le nombre de pièces du bien.  -->
										<xsd:element name="NBRE_PIECES" type="xsd:int" minOccurs="0"/>
										<!-- Le nombre de parkings du bien.  -->
										<xsd:element name="NBRE_PARKING" type="xsd:int" minOccurs="0"/>
										<!-- Le nombre de garages du bien.  -->
										<xsd:element name="NBRE_GARAGE" type="xsd:int" minOccurs="0"/>
										<!-- Surface du commerce.  -->
										<xsd:element name="SURFACE_COMMERCE" type="xsd:int"/>
										<!-- Surface de l’annexe du bien.  -->
										<xsd:element name="SURFACE_ANNEXE" type="xsd:int" minOccurs="0"/>
										<!-- Le fond de commerce possède-t-il un logement.  -->
										<xsd:element name="LOGEMENT" type="BoolType" minOccurs="0"/>
										<!-- Surface du logement.  -->
										<xsd:element name="SURFACE_LOGEMENT" type="xsd:int" minOccurs="0"/>
										<!-- Chiffre d’affaire HT.  -->
										<xsd:element name="CHIFFRE_AFFAIRE_HT" type="xsd:decimal"/>
										<!-- Bénéfice restitué.  -->
										<xsd:element name="BENEFICE_RECONSTITUE" type="xsd:decimal"/>
										<!-- Total de la commission.  -->
										<xsd:element name="TOTAL_COMMISSION" type="xsd:decimal"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="FORME_JURIDIQUE" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="RENTABILITE_CASH_FLOW" type="xsd:decimal" minOccurs="0"/>
										<!-- Date de création de l’affaire.  -->
										<xsd:element name="DATE_AFFAIRE" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Expiration du bail.  -->										
										<xsd:element name="BAIL_EXPIRATION" type="xsd:int"/>
										<!-- -->
										<xsd:element name="NBRE_PERSONNEL" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="TAUX_TAXES_PROFESSIONNELLES" type="xsd:decimal" minOccurs="0"/>
										<!-- -->
										<xsd:element name="ANNEE1" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="4" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Première année d’activité. Taille maximum : 4 caractères.  -->
										<xsd:element name="ANNEE1_CA_HT" type="xsd:decimal" minOccurs="0"/>
										<!-- Chiffre d’affaire de la première année d’activité.  -->
										<xsd:element name="ANNEE1_RESULTAT" type="xsd:decimal" minOccurs="0"/>
										<!-- Deuxième année d’activité .Taille maximum : 4 caractères.  -->
										<xsd:element name="ANNEE2" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="4" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="ANNEE2_CA_HT" type="xsd:decimal" minOccurs="0"/>
										<!-- Résultat de la deuxième année.  -->
										<xsd:element name="ANNEE2_RESULTAT" type="xsd:decimal" minOccurs="0"/>
										<!-- Troisième année d’activité .Taille maximum : 4 caractères.  -->
										<xsd:element name="ANNEE3" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="4" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="ANNEE3_CA_HT" type="xsd:decimal" minOccurs="0"/>
										<!-- Chiffre d’affaire de la troisième année d’activité.  -->
										<xsd:element name="ANNEE3_RESULTAT" type="xsd:decimal" minOccurs="0"/>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="CATEGORIE" type="categorie_fond_commerce"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Cette balise contient les informations génériques du bien location de vacances. Cette balise de bien doit être couplée à la balise PLANNING et TARIFICATION. Elle peut être contenue dans une balise BIEN -->
							<xsd:element name="VACANCES" minOccurs="0">
								<xsd:complexType>
									<xsd:all>
										<!-- (voir dictionnaire des références)  -->
										<xsd:element name="THEME" type="xsd:int"/>
										<!-- -->
										<xsd:element name="AVENANT_DEBUT" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- -->
										<xsd:element name="AVENANT_FIN" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="(\d{1,2})/(\d{1,2})/(\d{4}|\d{2})"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Nombre de pièces du bien.  -->
										<xsd:element name="NBRE_PIECES" type="xsd:string"/>
										<!-- Balise mère du groupe de balise <PIECE>. <PIECE> peut être instanciée au minimum une fois et au maximum d’après la valeur de la balise NBR_PIECES  -->
										<xsd:element name="GROUPE_PIECES" minOccurs="0">
	         										<xsd:complexType>
	           											<xsd:sequence>
													<!-- -->
													<xsd:element name="PIECE" maxOccurs="unbounded" minOccurs="0">
														<xsd:complexType>
															<xsd:all>
																<!-- Appartient au module de balise <PIECE>. Taille maximum : 30 caractères.  -->
																<xsd:element name="LIBELLE_PIECE">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:maxLength value="30" fixed="true"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<!-- Appartient au module de balise <PIECE>. Taille maximum : 5 caractères -->
																<xsd:element name="PIECE_EXPOSITION" minOccurs="0">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:maxLength value="5" fixed="true"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<!-- Appartient au module de balise <PIECE>.  -->
																<xsd:element name="PIECE_SURFACE" type="xsd:int" minOccurs="0"/>
																<!-- Appartient au module de balise <PIECE>. Le nombre de lits une place  -->
																<xsd:element name="PIECE_LIT_1_PLACE" type="xsd:int" minOccurs="0"/>
																<!-- Appartient au module de balise <PIECE>. Le nombre de lits deux places  -->
																<xsd:element name="PIECE_LIT_2_PLACES" type="xsd:int" minOccurs="0"/>
															</xsd:all>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
									          </xsd:complexType>
									        </xsd:element>
										<!-- Surface totale du bien.  -->
										<xsd:element name="SURFACE" type="xsd:int"/>
										<!-- Nombre de couchage total du bien.  -->
										<xsd:element name="NBRE_COUCHAGES" type="xsd:int" minOccurs="0"/>
										<!-- -->
										<xsd:element name="DEPOT_GARANTIE" type="BoolType" minOccurs="0"/>
										<!-- Taille maximum : 40 caractères.  -->
										<xsd:element name="TYPE_BIEN" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="40" fixed="true"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Animaux autorisé ou non.  -->
										<xsd:element name="INFO_ANIMAUX_AUTORISES" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="INFO_LIT_2_PLACES" type="BoolType" minOccurs="0"/>
										<!-- Existence d’un balcon ou non.  -->
										<xsd:element name="INFO_BALCON" type="BoolType" minOccurs="0"/>
										<!-- Existence d’une cour ou non.  -->
										<xsd:element name="INFO_COUR" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="INFO_FOUR" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="INFO_WC" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un lave-linge ?  -->
										<xsd:element name="INFO_LAVE_LINGE" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un parc ?  -->
										<xsd:element name="INFO_PARC" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un piscine ?  -->
										<xsd:element name="INFO_PISCINE" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un réfrigérateur ?  -->
										<xsd:element name="INFO_REFRIGERATEUR" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="INFO_LIT_1_PLACE" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un lit enfant ? -->
										<xsd:element name="INFO_LIT_ENFANT" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="INFO_COCOTTE" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="INFO_FER_REPASSER" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un garage ? -->
										<xsd:element name="INFO_GARAGE" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un jardin ? -->
										<xsd:element name="INFO_JARDIN" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un lave-vaisselle ? -->
										<xsd:element name="INFO_LAVE_VAISSELLE" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="INFO_PARKING" type="BoolType" minOccurs="0"/>
										<!-- -->
										<xsd:element name="INFO_RECHAUD" type="BoolType" minOccurs="0"/>
										<!-- Le bien possède-t-il un ascenseur ? -->
										<xsd:element name="ASCENSEUR" type="BoolType" minOccurs="0"/>
										<!-- Balise mère du groupe de balise <ALENTOURS>. Alentours peut apparaître 9 fois au maximum. Balises incluses dans ALENTOURS -->
										<xsd:element name="GROUPE_ALENTOURS" minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<!-- -->
													<xsd:element name="ALENTOURS" minOccurs="0" maxOccurs="9">
														<xsd:complexType>
															<xsd:sequence>
																<!-- Obligatoire dans la balise ALENTOURS. Description des alentours .Taille maximum : 30 caractères. -->
